{
  "name": "squirrel-math",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint-nofix-quiet": "vue-cli-service lint --no-fix --quiet",
    "test": "SET TS_NODE_COMPILER_OPTIONS={\"module\": \"commonjs\" } && mocha -r ts-node/register \"src/tests/**/*.ts\""
  },
  "dependencies": {
    "@types/jquery": "^3.3.31",
    "bootstrap-vue": "^2.0.4",
    "core-js": "^3.3.2",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "paper": "0.12.3",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/mocha": "^8.2.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-plugin-typescript": "^4.5.11",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "chai": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "mocha": "^8.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-console": "off",
      "vue/no-unused-components": "warning"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
